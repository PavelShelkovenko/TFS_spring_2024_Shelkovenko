<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EmojiReactionView">
        <attr name="reactionCount" format="integer"/>
        <attr name="emojiCode" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="selectedBackgroundColor" format="color"/>
        <attr name="unselectedBackgroundColor" format="color"/>
        <attr name="isSelectedReaction" format="boolean"/>
        <attr name="borderColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
    </declare-styleable>
    
    <attr name="emojiDefaultAttr" format="reference"/>


</resources>